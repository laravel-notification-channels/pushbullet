{
    "name": "laravel-notification-channels/pushbullet",
    "description": "Laravel 5.3+ Notifications Pushbullet Driver",
    "homepage": "https://github.com/laravel-notification-channels/pushbullet",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Plekhanov",
            "email": "alex@plekhanov.dev",
            "homepage": "https://alex.plekhanov.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.2 || ^7.0",
        "illuminate/notifications": "~5.5 || ~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~5.5 || ~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.0",
        "phpunit/phpunit": "^8.5.21 | ^9.4.4"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Pushbullet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Pushbullet\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Pushbullet\\PushbulletServiceProvider"
            ]
        }
    }
}
